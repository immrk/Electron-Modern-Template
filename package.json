{
  "name": "electron-study",
  "version": "1.0.0",
  "main": "./dist/main/main.js",
  "type": "module",
  "scripts": {
    "watch": "tsc --watch",
    "dev": "vite",
    "dev:main": "vite --port 11069",
    "dev:setting": "vite --port 11070",
    "dev:multi": "concurrently \"npm run dev:main\" \"npm run dev:setting\"",
    "build:renderer": "vite build",
    "start": "electron ./dist/main/main.js",
    "forge:start": "electron-forge start",
    "package": "electron-forge package",
    "make": "rm -rf dist && rm -rf out && tsc && npm run build:renderer && electron-forge make"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^24.0.8",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "concurrently": "^9.1.0",
    "electron": "^36.5.0",
    "vite": "^6.3.5",
    "vue": "^3.5.17"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
}
